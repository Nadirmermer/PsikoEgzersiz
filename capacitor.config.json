{
  "appId": "com.psikoegzersiz.app",
  "appName": "PsikoEgzersiz",
  "webDir": "dist",
  "server": {
    "androidScheme": "https"
  },
  "plugins": {
    "StatusBar": {
      "style": "default",
      "backgroundColor": "#6366f1"
    },
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#6366f1",
      "androidSplashResourceName": "splash",
      "iosSplashResourceName": "Default"
    },
    "Keyboard": {
      "resize": "body",
      "resizeOnFullScreen": true
    },
    "Device": {
      "orientation": "all"
    }
  },
  "android": {
    "allowMixedContent": true,
    "minWebViewVersion": 60,
    "webContentsDebuggingEnabled": true,
    "hideLogs": false,
    "orientations": ["portrait", "landscape", "portrait-reverse", "landscape-reverse"],
    "useLegacyBridge": false,
    "useFileApi": true
  },
  "ios": {
    "scheme": "PsikoEgzersiz",
    "contentInset": "automatic",
    "preferredContentMode": "mobile",
    "orientations": ["portrait", "landscape", "portrait-upside-down", "landscape-left", "landscape-right"],
    "handleApplicationURL": true,
    "backgroundColor": "#6366f1"
  }
}
